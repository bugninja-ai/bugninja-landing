{
  "collectionName": "components_shared_seos",
  "info": {
    "displayName": "SEO",
    "description": "Search Engine Optimization metadata"
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "required": true,
      "maxLength": 60
    },
    "metaDescription": {
      "type": "text",
      "required": true,
      "maxLength": 160
    },
    "keywords": {
      "type": "text"
    },
    "metaRobots": {
      "type": "string",
      "default": "index, follow"
    },
    "canonicalURL": {
      "type": "string"
    },
    "preventIndexing": {
      "type": "boolean",
      "default": false
    },
    "metaSocial": {
      "type": "component",
      "repeatable": true,
      "component": "shared.meta-social"
    },
    "structuredData": {
      "type": "json"
    }
  }
} 